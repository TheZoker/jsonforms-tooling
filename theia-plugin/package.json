{
  "name": "jsonforms-tooling-theia",
  "displayName": "JSONForms Tooling",
  "description": "Extension for the VS Code editor to support JSONForms Tooling, making it easier for developers to start new project or try out the library.",
  "version": "0.0.1",
  "publisher": "EclipseSource",
  "license": "MIT",
  "keywords": [
    "theia-plugin"
  ],
  "files": [
    "src"
  ],
  "dependencies": {
    "jsonforms-tooling": "file:../tooling",
    "@theia/plugin": "^0.4.0-next.8c668434",
    "@theia/plugin-packager": "0.0.1-1539891078"
  },
  "devDependencies": {
    "@types/node": "^10.12.10",
    "rimraf": "2.6.2"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf out",
    "watch": "tsc -watch",
    "compile": "tsc -p ./",
    "build": "npm run compile && theia:plugin pack",
    "postinstall": " "
  },
  "engines": {
    "theiaPlugin": "next"
  },
  "theiaPlugin": {
    "backend": "out/extension.js"
  }
}
